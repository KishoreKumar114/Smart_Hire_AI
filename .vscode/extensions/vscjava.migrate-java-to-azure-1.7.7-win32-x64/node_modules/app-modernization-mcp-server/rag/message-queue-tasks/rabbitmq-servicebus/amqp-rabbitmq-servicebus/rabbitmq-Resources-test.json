      "import org.springframework.amqp.rabbit.core.RabbitTemplate;",
      "import com.rabbitmq.client.Channel;",
      "String rabbitMQHost = \"localhost\";"
    ]
  }
  ,
  "fullTextSearch": {
    "shouldMatch": [
      "Migrate RabbitMQ resource creation to Azure Service Bus resources",
      "Migrate Spring Bean configurations of RabbitMQ resources to Azure Service Bus resource beans",
      "Map RabbitMQ Queue to QueueProperties, Exchange to TopicProperties, Binding to SubscriptionProperties",
      "Migrate AmqpAdmin or RabbitAdmin to ServiceBusAdministrationClient",
      "Autowire AzureServiceBusProperties and TokenCredential for ServiceBusAdministrationClient",
      "Migrate RabbitListenerContainerFactory to PropertiesSupplier<ConsumerIdentifier, ProcessorProperties>",
      "Add PropertiesSupplier<String, ProducerProperties> for customized RabbitTemplate properties",
      "Remove all amqp imports after migration"
    ],
    "shouldNotMatch": [
      "Unrelated resource migration",
      "Migrate to non-Azure message queue",
      "Keep using RabbitMQ resources without migration"
    ]
  }
