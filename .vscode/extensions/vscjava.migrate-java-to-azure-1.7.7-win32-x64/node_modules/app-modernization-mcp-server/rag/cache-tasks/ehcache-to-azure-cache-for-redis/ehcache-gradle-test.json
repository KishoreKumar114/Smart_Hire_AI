{
  "filePattern": {
    "shouldMatch": [
      "build.gradle",
      "nested/build.gradle.kts"
    ],
    "shouldNotMatch": [
      "pom.xml",
      "src/ExampleClass.java",
      "README.md"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "implementation 'net.sf.ehcache:ehcache:2.10.6'",
      "implementation 'net.sf.ehcache:ehcache-core:2.6.11'",
      "implementation 'org.ehcache:ehcache:3.9.0'",
      "implementation 'org.springframework.boot:spring-boot-starter-cache'",
      "implementation(\"net.sf.ehcache:ehcache:2.10.6\")",
      "implementation(\"net.sf.ehcache:ehcache-core:2.6.11\")",
      "implementation(\"org.ehcache:ehcache:3.9.0\")",
      "implementation(\"org.springframework.boot:spring-boot-starter-cache\")"
    ],
    "shouldNotMatch": [
      "implementation 'com.azure.spring:azure-spring-data-redis:3.19.0'",
      "implementation(\"com.azure.spring:azure-spring-data-redis:3.19.0\")",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
  ,
  "fullTextSearch": {
    "shouldMatch": [
      "Migrate Ehcache Gradle dependencies to Azure Cache for Redis",
      "Replaces Ehcache Gradle dependencies with Azure Cache for Redis dependencies",
      "How to migrate Ehcache Gradle dependencies to Azure Cache for Redis using Groovy or Kotlin DSL",
      "Update implementation statements for net.sf.ehcache:ehcache and org.ehcache:ehcache to Azure Redis equivalents",
      "Remove or replace Ehcache dependencies and add Azure Cache for Redis dependencies in build.gradle or build.gradle.kts",
      "Example migration of org.springframework.boot:spring-boot-starter-cache to spring-boot-starter-data-redis",
      "Provide a valid, complete Gradle build file after migration"
    ],
    "shouldNotMatch": [
      "unrelated gradle dependency",
      "remove all gradle dependencies"
    ]
  }
}
