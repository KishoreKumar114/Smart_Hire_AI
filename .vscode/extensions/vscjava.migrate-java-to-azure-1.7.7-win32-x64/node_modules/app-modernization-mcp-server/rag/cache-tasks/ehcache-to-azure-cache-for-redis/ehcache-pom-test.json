{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "subdir/project/pom.xml"
    ],
    "shouldNotMatch": [
      "build.gradle",
      "nested/build.gradle",
      "subdir/project/build.gradle"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<dependency>\n  <groupId>net.sf.ehcache</groupId>\n  <artifactId>ehcache</artifactId>\n  <version>2.10.6</version>\n</dependency>",
      "<dependency>\n  <groupId>net.sf.ehcache</groupId>\n  <artifactId>ehcache-core</artifactId>\n  <version>2.6.11</version>\n</dependency>",
      "<dependency>\n  <groupId>org.springframework.boot</groupId>\n  <artifactId>spring-boot-starter-cache</artifactId>\n</dependency>",
      "<dependency>\n  <groupId>org.springframework.boot</groupId>\n  <groupId>net.sf.ehcache<\/groupId>\n</dependency>"
    ],
    "shouldNotMatch": [
      "<dependency>\n  <groupId>com.example</groupId>\n  <artifactId>unrelated-dependency</artifactId>\n  <version>1.0.0</version>\n</dependency>",
      "<dependency>\n  <groupId>org.apache.activemq</groupId>\n  <artifactId>activemq-core</artifactId>\n  <version>5.15.9</version>\n</dependency>"
    ]
  }
  ,
  "fullTextSearch": {
    "shouldMatch": [
      "Migrate Ehcache Maven dependencies to Azure Cache for Redis",
      "Replaces Ehcache Maven dependencies with Azure Cache for Redis dependencies",
      "How to migrate Ehcache Maven dependencies to Azure Cache for Redis in pom.xml files",
      "Update <dependency> blocks for net.sf.ehcache:ehcache and org.springframework.boot:spring-boot-starter-cache to Azure Redis equivalents",
      "Remove or replace Ehcache dependencies and add Azure Cache for Redis dependencies in pom.xml",
      "Example migration of <artifactId>ehcache</artifactId> and <artifactId>spring-boot-starter-cache</artifactId> to <artifactId>spring-boot-starter-data-redis</artifactId>",
      "Provide a valid, complete pom.xml file after migration"
    ],
    "shouldNotMatch": [
      "unrelated maven dependency",
      "remove all maven dependencies"
    ]
  }
}
