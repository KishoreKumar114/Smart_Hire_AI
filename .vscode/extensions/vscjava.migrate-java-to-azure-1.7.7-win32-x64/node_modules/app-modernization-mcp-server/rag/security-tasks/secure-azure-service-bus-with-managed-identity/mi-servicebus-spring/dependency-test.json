{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "build.gradle",
      "nested/build.gradle"
    ],
    "shouldNotMatch": [
      "Application.java",
      "application.properties",
      "config.yaml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>azure-messaging-servicebus</artifactId>",
      "<artifactId>spring-messaging-azure-servicebus</artifactId>",
      "implementation group: 'com.azure', name: 'azure-messaging-servicebus', version: \"${azureServiceBusVersion}\"",
      "implementation 'com.azure.spring:spring-messaging-azure-servicebus:5.0.0'"
    ],
    "shouldNotMatch": [
      "<artifactId>azure-storage-queue</artifactId>",
      "testImplementation 'com.azure:azure-data-tables:12.5.0'"
    ]
    },
    "fullTextSearch": {
      "shouldMatch": [
        "How do I migrate Spring Service Bus authentication to use Azure Managed Identity?",
        "What dependency changes are needed for managed identity support in Spring Service Bus?"
      ]
    }
}
