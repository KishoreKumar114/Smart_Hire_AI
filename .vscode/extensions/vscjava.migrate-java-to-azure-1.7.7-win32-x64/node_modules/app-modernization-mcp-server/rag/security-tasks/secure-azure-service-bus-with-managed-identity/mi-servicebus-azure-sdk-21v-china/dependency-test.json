{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "build.gradle",
      "nested/build.gradle"
    ],
    "shouldNotMatch": [
      "Application.java",
      "application.properties",
      "config.yaml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>azure-messaging-servicebus</artifactId>",
      "implementation group: 'com.azure', name: 'azure-messaging-servicebus', version: \"${azureServiceBusVersion}\""
    ],
    "shouldNotMatch": [
      "<artifactId>azure-storage-queue</artifactId>",
      "testImplementation 'com.azure:azure-data-tables:12.5.0'"
    ]
  },
  "fullTextSearch": {
    "shouldMatch": [
      "How do I migrate Service Bus SDK authentication from connection string to Azure Managed Identity in Java?",
      "What changes are needed in Maven/Gradle dependencies for managed identity support in Service Bus?"
    ]
  }
}
