{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/build.gradle",
      "nested/build.gradle.kts"
    ],
    "shouldNotMatch": [
      "src/ExampleClass.java",
      "README.md"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>secretsmanager</artifactId>",
      "<artifactId>aws-java-sdk-secretsmanager</artifactId>",
      "implementation 'software.amazon.awssdk:secretsmanager'",
      "implementation(\"software.amazon.awssdk:secretsmanager\")",
      "implementation 'com.amazonaws:aws-java-sdk-secretsmanager'",
      "implementation(\"com.amazonaws:aws-java-sdk-secretsmanager\")"
    ],
    "shouldNotMatch": [
      "implementation 'com.azure:azure-security-keyvault-secrets:4.9.3'",
      "implementation(\"com.azure:azure-security-keyvault-secrets:4.9.3\")",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
  ,
  "fullTextSearch": {
    "shouldMatch": [
      "Migrate AWS Secrets Manager pom dependency to Azure Key Vault",
      "Replace AWS Secrets Manager dependencies with Azure Key Vault dependencies",
      "Always provide Azure Identity dependency for passwordless authentication"
    ],
    "shouldNotMatch": [
      "Unrelated dependency migration",
      "Migrate to non-Azure secrets service",
      "Keep using AWS Secrets Manager dependencies without migration"
    ]
  }
}
