{
  "name": "starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prebuild && nest start --watch",
    "start:debug": "npm run prebuild && nest start --debug 0.0.0.0:9229 --watch --tsc",
    "start:prod": "node dist/main",
    "lint": "npx tsc --project tsconfig.json --noEmit --skipLibCheck && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:prepare": "mkdir -p dev/mysql/data",
    "dev:start": "npm run dev:prepare && docker compose up -d --build",
    "dev:stop": "docker compose down --remove-orphans",
    "dev:restart": "npm run dev:stop && npm run dev:start",
    "migration:generate:dev": "docker compose exec starter npx typeorm-ts-node-esm migration:generate -d ./src/orm.ts",
    "migration:run:dev": "docker compose exec starter npx typeorm-ts-node-esm migration:run -d ./src/orm.ts",
    "migration:revert:dev": "docker compose exec starter npx typeorm-ts-node-esm migration:revert -d ./src/orm.ts",
    "migration:run:prod": "npx typeorm migration:run -d dist/orm.js",
    "bitbucket:env:generate": "scripts_generators/bitbucket_env_generator.sh",
    "aws:scripts:generate": "scripts_generators/aws_scripts_generator.sh",
    "rebase": "bash ./dev/script/rebase.sh"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@types/mime-types": "^2.1.1",
    "archiver": "^6.0.1",
    "aws-sdk": "^2.1297.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.7",
    "dotenv": "^16.4.5",
    "dotenv-vault-core": "^0.6.1",
    "gltf-import-export": "^1.0.12",
    "image-pal": "^1.2.2",
    "ip": "^1.1.8",
    "jsforce": "^1.11.0",
    "mailgun.js": "^8.0.6",
    "md5": "^2.3.0",
    "memoizee": "^0.4.15",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.23",
    "nj-request-scope": "^1.0.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pretty-error": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.3",
    "short-unique-id": "^5.0.3",
    "ssh2-sftp-client": "^9.1.0",
    "timekeeper": "^2.2.0",
    "typeorm": "^0.3.10",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/archiver": "^5.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.3",
    "@types/jest": "28.1.8",
    "@types/jsforce": "^1.11.0",
    "@types/md5": "^2.3.2",
    "@types/memoizee": "^0.4.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.2.5",
    "jest": "28.1.3",
    "lint-staged": "^10.0.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^2.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": "rm -f .env.previous"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  }
}
